{
  "root": true,
  "env": {
    "browser": true,
    "node": true
  },
  "parserOptions": {
    "parser": "@babel/eslint-parser",
    "requireConfigFile": false
  },
  "extends": ["@nuxtjs", "plugin:nuxt/recommended", "prettier"],
  "rules": {
    "no-console": "off",
    "camelcase": 0,
    "no-unused-vars": "warn",
    "space-before-function-paren": 0,
    "template-curly-spacing": 0,
    "vue/no-v-html": 0
  }
}
